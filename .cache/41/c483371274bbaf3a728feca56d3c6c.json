        <br />\n        YOUR HEAD\n      </StyledFooterLogo>\n      <StyledFooterRight>\n        <StyledFooterNav>\n          <span>Subscribe:</span>\n          <StyledFooterLinks>\n            {links\n              ? links.map((link, index) => {\n                  return (\n                    <StyledFooterItem key={index}>\n                      <StyledFooterLink href={link.href}>\n                        {link.text}\n                      </StyledFooterLink>\n                    </StyledFooterItem>\n                  );\n                })\n              : null}\n          </StyledFooterLinks>\n        </StyledFooterNav>\n      </StyledFooterRight>\n    </StyledArticleFooter>\n  );\n};\nexport default ArticleFooter;\n"},"lineCount":null}},"error":null,"hash":"1b34e0b260f8486f17cd938b932ada5c","cacheData":{"env":{}}}